{
  "name": "startster",
  "version": "0.3",
  "description": "An beautiful start page for your browser",
  "manifest_version": 2,
  "options_page": "options.html",
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab" : "newtab.html"
  },
  "update_url": "https://raw.githubusercontent.com/piekay/startster/master/chrome/updates.xml",
  "permissions": [
    "storage"
  ]
}