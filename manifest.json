{
  "name": "startster",
  "version": "1.0",
  "description": "An optimized Startpage for Searching and other stuff!",
  "manifest_version": 2,
  "options_page": "options.html",
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab" : "newtab.html"
  },
  "permissions": [
    "storage"
  ]
}